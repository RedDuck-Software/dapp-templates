{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "target": "ES2019",
    "lib": ["ES2019", "DOM"],
    "module": "ES6",
    "moduleResolution": "Node",
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  },
  "exclude": ["./node_modules"]
}
